@misc{korfmannSimultaneousInferenceDemography2023,
  title = {Simultaneous {{Inference}} of {{Past Demography}} and {{Selection}} from the {{Ancestral Recombination Graph}} under the {{Beta Coalescent}}},
  author = {Korfmann, Kevin and Sellinger, Thibaut and Freund, Fabian and Fumagalli, Matteo and Tellier, Aur{\'e}lien},
