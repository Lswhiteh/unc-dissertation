@article{gaoNewSoftwareFast2016,
  title = {New {{Software}} for the {{Fast Estimation}} of {{Population Recombination Rates}} ({{FastEPRR}}) in the {{Genomic Era}}},
  author = {Gao, Feng and Ming, Chen and Hu, Wangjie and Li, Haipeng},
  year = {2016},
