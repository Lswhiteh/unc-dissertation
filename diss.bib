  keywords = {Alleles,Europe,Gene mapping,Genomic signal processing,Genomics,Haplotypes,notion,Notion,Simulation and modeling,Single nucleotide polymorphisms,Timesweeper},
  file = {C\:\\Users\\lwhit\\Zotero\\storage\\5KQCMZHC\\Voight et al. - 2006 - A Map of Recent Positive Selection in the Human Ge.pdf;C\:\\Users\\lwhit\\Zotero\\storage\\RJ5GEJ9J\\article.html}
}

@article{voznicaDeepLearningPhylogenies2022,
  title = {Deep Learning from Phylogenies to Uncover the Epidemiological Dynamics of Outbreaks},
  author = {Voznica, J. and Zhukova, A. and Boskova, V. and Saulnier, E. and Lemoine, F. and {Moslonka-Lefebvre}, M. and Gascuel, O.},
