@article{hermissonSoftSweepsUnderstanding2017,
  title = {Soft Sweeps and beyond: Understanding the Patterns and Probabilities of Selection Footprints under Rapid Adaptation},
  shorttitle = {Soft Sweeps and Beyond},
  author = {Hermisson, Joachim and Pennings, Pleuni S.},
  year = {2017},
