@article{jianPredictabilityMosquitoAbundance2016,
  title = {The Predictability of Mosquito Abundance from Daily to Monthly Timescales},
  author = {Jian, Yun and Silvestri, Sonia and Brown, Jeff and Hickman, Rick and Marani, Marco},
  year = {2016},
  journal = {Ecological Applications},
  volume = {26},
  number = {8},
