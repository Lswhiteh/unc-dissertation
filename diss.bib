@article{schriderSupervisedMachineLearning2018a,
  title = {Supervised Machine Learning Reveals Introgressed Loci in the Genomes of {{Drosophila}} Simulans and {{D}}. Sechellia},
  author = {Schrider, Daniel R. and Ayroles, Julien and Matute, Daniel R. and Kern, Andrew D.},
