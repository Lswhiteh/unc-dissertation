@article{fanLikelihoodbasedFrameworkDemographic2023,
  title = {A Likelihood-Based Framework for Demographic Inference from Genealogical Trees},
  author = {Fan, Caoqi and Cahoon, Jordan L. and Dinh, Bryan L. and {Ortega-Del Vecchyo}, Diego and Huber, Christian and Edge, Michael D. and Mancuso, Nicholas and Chiang, Charleston W. K.},
  year = {2023},
