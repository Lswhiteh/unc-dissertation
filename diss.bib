@misc{pelletierComplexitiesRecapitulatingPolygenic2022,
  title = {Complexities of Recapitulating Polygenic Effects in Natural Populations: Replication of Genetic Effects on Wing Shape in Artificially Selected and Wild Caught Populations of {{Drosophila}} Melanogaster},
  shorttitle = {Complexities of Recapitulating Polygenic Effects in Natural Populations},
  author = {Pelletier, Katie and Pitchers, William R. and Mammel, Anna and {Northrop-Albrecht}, Emmalee and M{\'a}rquez, Eladio J. and Moscarella, Rosa A. and Houle, David and Dworkin, Ian},
