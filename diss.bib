@article{xueDiscoveryOngoingSelective2021,
  title = {Discovery of {{Ongoing Selective Sweeps}} within {{Anopheles Mosquito Populations Using Deep Learning}}},
  author = {Xue, Alexander T. and Schrider, Daniel R. and Kern, Andrew D. and {Ag1000g Consortium}},
  year = {2021},
